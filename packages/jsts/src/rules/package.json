  },
  "files": [
    "README.md",
    "LICENSE",
    "lib"
  ],
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0"
  },
  "homepage": "https://github.com/SonarSource/SonarJS#readme",
  "dependencies": {
    "@babel/core": "7.24.3",
    "@babel/eslint-parser": "7.24.1",
